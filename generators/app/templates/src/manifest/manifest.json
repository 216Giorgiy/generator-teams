{
    "$schema": "https://statics.teams.microsoft.com/sdk/v1.0/manifest/MicrosoftTeams.schema.json",
    "manifestVersion": "1.0",
    "id": "<%= id %>",
    "version": "0.0.1",
    "packageName": "<%= namespace %>",
    "developer": {
        "name": "<%=developer %>",
        "websiteUrl": "<%=host %>",
        "privacyUrl": "<%=privacy %>",
        "termsOfUseUrl": "<%=tou %>"
    },
    "name": {
        "short": "<%=title%>"
    },
    "description": {
        "short": "<%=description%>"
    },
    "icons": {
        "outline": "icon-20x20.png",
        "color": "icon-96x96.png"
    },
    "accentColor": "#FFFFFF",
    "configurableTabs": [],
    "staticTabs": [],
    "bots": [],
    "connectors": [],
    "composeExtensions": [],
    "needsIdentity": true,
    "permissions": ["identity", "messageTeamMembers"],
    "validDomains": []
}